<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:background="#2f333f"

    >




    <LinearLayout
        android:background="#00695c"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:layout_width="wrap_content"
        android:layout_height="100dp">
        <com.vipul.hp_hp.timelineview.TimelineView
            app:marker="@drawable/manwhite"
            android:paddingStart="30dp"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            app:marker_size="30dp"
            app:line_size="1dp"
            app:line="#ffff"/>
        <TextView
            android:id="@+id/wedcse424"


            android:textColor="@color/wh"
            android:text="CSE-424"
            android:textSize="20sp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:ignore="OnClick" />
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:textColor="@color/wh"
                android:text="11.00am"
                android:textSize="20dp"
                android:paddingLeft="20dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />
            <TextView
                android:textColor="@color/wh"
                android:text="1.00pm"
                android:textSize="20dp"
                android:paddingLeft="20dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />


        </LinearLayout>
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:textColor="@color/wh"
                android:text="FI+BM"
                android:textSize="20dp"
                android:paddingLeft="20dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />
            <TextView
                android:textColor="@color/wh"
                android:text="Room-SEL"
                android:textSize="20dp"
                android:paddingLeft="20dp"
                android:layout_width="200dp"
                android:layout_height="wrap_content" />


        </LinearLayout>





    </LinearLayout>
    <LinearLayout
        android:background="#1faa00"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:layout_width="wrap_content"
        android:layout_height="100dp">
        <com.vipul.hp_hp.timelineview.TimelineView
            app:marker="@drawable/manwhite"
            android:paddingStart="30dp"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            app:marker_size="30dp"
            app:line_size="1dp"
            app:line="#ffff"/>
        <TextView
            android:id="@+id/wedcse425"
            android:textColor="@color/wh"
            android:text="CSE-425"
            android:textSize="20sp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:textColor="@color/wh"
                android:text="3.00pm"
                android:textSize="20dp"
                android:paddingLeft="20dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />
            <TextView
                android:textColor="@color/wh"
                android:text="4.25pm"
                android:textSize="20dp"
                android:paddingLeft="20dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />


        </LinearLayout>
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingLeft="20dp"
                android:text="SN"
                android:textColor="@color/wh"
                android:textSize="20dp" />
            <TextView
                android:textColor="@color/wh"
                android:text="Room-501"
                android:textSize="20dp"
                android:paddingLeft="20dp"
                android:layout_width="200dp"
                android:layout_height="wrap_content" />


        </LinearLayout>





    </LinearLayout>
    <LinearLayout
      android:background="#c43c00"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:layout_width="wrap_content"
        android:layout_height="100dp">
        <com.vipul.hp_hp.timelineview.TimelineView
            app:marker="@drawable/manwhite"
            android:paddingStart="30dp"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            app:marker_size="20dp"
            app:line_size="1dp"
            app:line="#ffff"/>
        <TextView
            android:id="@+id/wedcse421"
            android:textColor="@color/wh"
            android:text="CSE-421"
            android:textSize="20sp"
            android:layout_width="wrap_content"
